{
  "version": 2,
  "builds": [
    {
      "src": "public/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["public/**"] }
    }
  ],
  "buildCommand": "cd apps/backend && bun run build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.js"
    }
  ]
}
